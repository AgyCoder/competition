<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.competition.competitionsys.dao.UserDao">
    <select id="findAllUsers" resultType="com.competition.competitionsys.domain.UserModel">
        select * from user
    </select>

    <select id="findUserByUserCode" resultType="com.competition.competitionsys.domain.UserModel" parameterType="Integer">
        select * from user where userCode = #{userCode}
    </select>
</mapper>